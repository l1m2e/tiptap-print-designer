{
  "name": "tiptap-print-designer",
  "type": "module",
  "version": "0.0.36",
  "private": false,
  "description": "一个开箱即用的富文本打印设计器，基于 tiptap 和 Vue3 开发",
  "author": "l1m2e",
  "license": "MIT",
  "homepage": "https://github.com/l1m2e/tiptap-print-designer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/l1m2e/tiptap-print-designer.git"
  },
  "bugs": {
    "url": "https://github.com/l1m2e/tiptap-print-designer/issues"
  },
  "keywords": [
    "tiptap",
    "vue3",
    "print",
    "designer",
    "rich-text",
    "template",
    "editor"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./index.css": "./dist/index.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build && pnpm copy-files",
    "copy-files": "node scripts/copy-files.js",
    "clean-copied-files": "node scripts/clean-copied-files.js",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc",
    "preview": "vite preview",
    "up": "taze major -I",
    "prepublishOnly": "pnpm run build",
    "publish:npm": "npm publish"
  },
  "peerDependencies": {
    "@floating-ui/dom": "1.6.0",
    "@tiptap/core": "3.4.3",
    "@tiptap/extension-table": "3.4.3",
    "@tiptap/extension-text-align": "3.4.3",
    "@tiptap/extension-text-style": "3.4.3",
    "@tiptap/extensions": "3.4.3",
    "@tiptap/pm": "3.4.3",
    "@tiptap/starter-kit": "3.4.3",
    "@tiptap/suggestion": "3.4.3",
    "@tiptap/vue-3": "3.4.3",
    "monaco-editor": "0.52.0",
    "vue": "3.5.13"
  },
  "peerDependenciesMeta": {
    "monaco-editor": {
      "optional": true
    }
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.1.1",
    "@dagrejs/dagre": "1.1.4",
    "@shikijs/monaco": "3.2.2",
    "@tanstack/vue-table": "8.21.3",
    "@vee-validate/zod": "4.15.0",
    "@vueuse/components": "13.1.0",
    "@vueuse/core": "13.1.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dexie": "4.0.11",
    "lucide-vue-next": "0.501.0",
    "openapi-types": "12.1.3",
    "panzoom": "9.4.3",
    "postcss": "8.5.3",
    "radash": "12.1.0",
    "reka-ui": "2.2.0",
    "shiki": "3.2.2",
    "tailwind-merge": "2.6.0",
    "tailwindcss-animate": "1.0.7",
    "uuid": "11.1.0",
    "vaul-vue": "0.4.1",
    "vee-validate": "4.15.0",
    "vue-draggable-plus": "0.6.0",
    "vue-to-print": "1.4.0",
    "vue3-sfc-loader-chrome64": "0.9.5",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitejs/plugin-vue-jsx": "4.1.2",
    "autoprefixer": "10.4.21",
    "jsdom": "26.1.0",
    "lint-staged": "15.5.1",
    "monaco-editor": "0.52.0",
    "pnpm": "10.8.1",
    "tailwindcss": "3.4.17",
    "taze": "19.0.4",
    "typescript": "5.8.3",
    "unplugin-auto-import": "19.1.2",
    "unplugin-vue-components": "28.5.0",
    "vite": "6.3.2",
    "vite-plugin-dts": "4.5.3",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-node-polyfills": "0.23.0",
    "vue": "3.5.13",
    "vue-tsc": "2.2.8"
  }
}
